package com.breno.temporaries;

public class Node {

	Node right = null;
	Node left = null;
	int data;
	
	Node(int data) {
		this.data = data;
	}

	public Node add(int data) {
		if (left == null && data > data) {
			return new Node(data);
		} else if (right == null && data < data) {
			return new Node(data);
		}
	
		if (this.data > data) {
			left.add(data);
		} else if (this.data < data) {
			right.add(data);
		} else {
			return this; // se head.data == data
		}
	}
}

