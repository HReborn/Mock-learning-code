package com.breno.temporaries;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class TestTemp<T> {
	

	public static void main(String[] args) {

		Integer[] arr = {20, 30, 780 , 900 ,10, 15, 7, 120, 40, -30, -20, 400, 37, 128, 119, 40};
		int lastSwap = -1;
		int iterationEnd = arr.length - 1;
		int temp = 0;
		int swapCounter = 1;

		while (swapCounter != 0) {
			swapCounter = 0;
			for (int i = 0; i < iterationEnd; i++) {
				if (arr[i] > arr[i+1]) {
					swapCounter++;
					lastSwap = i;
					temp = arr[i+1];
					arr[i+1] = arr[i];
					arr[i] = temp;
				}
			}
			iterationEnd = lastSwap;
		}
		
		for (int i : arr) {
			System.out.print(arr);
		}
		System.out.println();
	}	
}